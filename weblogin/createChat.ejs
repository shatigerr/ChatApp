<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Create a chat!!</title>
    <link rel="stylesheet" href="./css/createChat.css">
</head>
<body>
    <nav>
        <ul class="nav-ul">
            <img src="./imatges/whatsapp.png" alt="">
            <div>
                <li><a href="">General Chat</a></li>
                <li><a href="">Add User</a></li>
            </div>
            
        </ul>
    </nav>

    <section class="users-select">
        <ul>
            <% results.forEach(function(user){ %>
            <li><%= user.username %> <a href="/chat/<%= user.id %>"><button type="button">Click</button></a> </li>
            <% }); %>
        </ul>
    </section>
</body>
</html>